{
    "registry" :[
        { "path": "inbox/.*" , 
          "with": {
            "url": "inbox/",
            "inbox": "./inbox",
            "inboxPublic": 1,
            "inboxWritable": 1
          }
        },
        { 
            "path": "trace[\/]?.*" , 
            "do": "./handler/tracelog.js" 
        }
    ]
}